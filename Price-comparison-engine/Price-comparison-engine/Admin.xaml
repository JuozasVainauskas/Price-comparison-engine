<Window x:Class="Price_comparison_engine.Admin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Price_comparison_engine"
        mc:Ignorable="d"
        Title="Admin" Height="450" Width="800">
    <Grid>
        <ComboBox x:Name="RolesPriskirimas" HorizontalAlignment="Left" Margin="175,44,0,0" VerticalAlignment="Top" Width="131">
            <ComboBoxItem Content="Vartotojas"/>
            <ComboBoxItem Content="Administratorius"/>
        </ComboBox>
        <TextBox x:Name="email" HorizontalAlignment="Left" Height="23" Margin="39,44,0,0" TextWrapping="Wrap" Text="Email" VerticalAlignment="Top" Width="120"/>
        <Button Content="Priskirti" HorizontalAlignment="Left" Margin="327,48,0,0" VerticalAlignment="Top" Width="75" Click="Priskirti"/>

    </Grid>
</Window>
